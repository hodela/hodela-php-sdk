{
    "name": "hodela/php-sdk",
    "description": "Hodela SDK for PHP",
    "keywords": ["hodela", "sdk"],
    "type": "library",
    "homepage": "https://github.com/hodela/hodela-php-sdk",
    "license": "Hodela Platform",
    "authors": [
        {
            "name": "Hodela",
            "homepage": "https://github.com/hodela/hodela-php-sdk/graphs/contributors"
        }
    ],
    "require": {
        "php": "^5.4|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.8"
    },
    "autoload": {
        "psr-4": {
            "Hodela\\": "src/Hodela/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hodela\\Tests\\": "tests/"
        }
    }
}